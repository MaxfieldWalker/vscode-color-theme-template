{
    "name": "example",
    "tokenColors": [
        {
            "name": "String",
            "scope": [
                "string",
                "constant.character.escape.ts"
            ],
            "settings": {
                "foreground": "$$string-color"
            }
        },
        {
            "name": "alpha value alias",
            "settings": {
                "foreground": "#0000FF$$medium"
            }
        },
        {
            "name": "whole alias",
            "settings": {
                "foreground": "$$list-active-background"
            }
        },
        {
            "name": "alias and alpha variable",
            "settings": {
                "foreground": "$$string-color@mid"
            }
        }
    ],
    "colorPalette": {
        "$blue": "#0000FF",
        "$green": "#0F0",
        "@mid": "80"
    },
    "alias": {
        "$$string-color": "$blue",
        "$$medium": "@mid",
        "$$list-active-background": "$blue@mid"
    }
}